{
  "name": "tabsets-pro",
  "version": "0.1.2",
  "description": "Use tabsets.net to safely keep track of all your tabs and links in the Side Panel, stored locally on your browser",
  "productName": "Tabsets Pro (Alpha!)",
  "author": "evandor <evandor@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "cp envs/env.dev ./.env && quasar dev -m bex -T chrome",
    "dev emulator": "cp envs/env.emulator ./.env && quasar dev -m bex -T chrome",
    "dev pwa emulator": "cp envs/env.emulator ./.env && quasar dev -m pwa",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "build": "cp envs/env.dev ./.env && quasar build -m bex -T chrome",
    "build pwa": "cp envs/env.dev ./.env && quasar build -m pwa",
    "build pwa.prd & deploy to india032": "cp envs/env.prd ./.env && quasar build -m pwa && scp -r -P 222 dist/pwa/* carsten@carsten.evandor.de:/home/carsten/tabsets/pwa.tabsets.net",
    "build pwa.dev & deploy to india032": "cp envs/env.dev ./.env && quasar build -m pwa && scp -r -P 222 dist/pwa/* carsten@carsten.evandor.de:/home/carsten/tabsets/pwa.dev.tabsets.net",
    "icon": "icongenie generate -i src/assets/icon.png",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "postinstall": "quasar prepare",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "npx playwright test",
    "test:e2e ui": "npx playwright test --ui",
    "test:e2e:showReport": "npx playwright show-report",
    "test:specificTest": "npx playwright test addFirstTabset",
    "test:specificTest:debug": "npx playwright test addFirstTabset --debug",
    "firebase build & deploy pwa (dev)": "cp envs/env.dev ./.env && quasar build -m pwa && firebase deploy --only hosting",
    "info": "yarn list > docs/yarn.list.txt && quasar info > docs/quasar.info.txt",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org skysail-dk --project tabsets-pro ./dist && sentry-cli sourcemaps upload --org skysail-dk --project tabsets-pro ./dist"
  },
