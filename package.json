{
  "name": "tabsets-pro",
  "version": "0.0.3",
  "description": "Use tabsets.net to safely keep track of all your tabs and links in the Side Panel, stored locally on your browser",
  "productName": "Tabsets Pro (Alpha!)",
  "author": "Carsten Gr√§f <evandor@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m bex",
    "dev pwa": "quasar dev -m pwa",
    "build bex (prd)": "mv .env .env.tmp && cp .prd.env .env && quasar build -m bex && mv .env.tmp .env",
    "build pwa": "quasar build -m pwa",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "build": "quasar build",
    "firebase build & deploy pwa": "quasar build -m pwa && firebase deploy --only hosting",
    "info": "yarn list > docs/yarn.list.txt && quasar info > docs/quasar.info.txt",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org skysail-dk --project tabsets-pro ./dist && sentry-cli sourcemaps upload --org skysail-dk --project tabsets-pro ./dist"
  },
